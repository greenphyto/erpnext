 "html": "{% if letter_head %}\r\n    {{ letter_head }}\r\n{% endif %}\r\n<p style=\"margin-top: 1.5rem; margin-bottom: 1rem\">\r\n\t<b style=\"font-size: 1.5rem;\">{{ doc.select_print_heading or _(\"Permit to Work\") }}</b><br>\r\n\t{{ doc.name }}\r\n</p>\r\n<p class=\"text-center\" style=\"margin-bottom: 1rem\">\r\n\t<b style=\"font-size: 1rem;\">{{ doc.workflow_state }}</b><br>\r\n</p>\r\n<p style=\"margin-bottom: 0.3rem\">\r\n\t<b style=\"font-size: 1rem;\">Details</b><br>\r\n</p>\r\n<div class=\"row row-no-gutters\">\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Item Description:</div>\r\n    <div class=\"col-xs-9\">{{ doc.item_description }}</div>\r\n</div>\r\n<div class=\"row row-no-gutters\">\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Size:</div>\r\n    <div class=\"col-xs-3\">{{ doc.size }}</div>\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Weight:</div>\r\n    <div class=\"col-xs-3\">{{ doc.weight }}</div>\r\n</div>\r\n<div class=\"row row-no-gutters\">\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Supplier Contact/ Greenphyto PIC Name:</div>\r\n    <div class=\"col-xs-3\">{{ doc.supplier_contact_name }}</div>\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Supplier Contact/ Greenphyto PIC No.:</div>\r\n    <div class=\"col-xs-3\">{{ doc.supplier_contact_phone }}</div>\r\n</div>\r\n<p style=\"margin-top: 1rem; margin-bottom: 0.3rem\">\r\n\t<b style=\"font-size: 1rem;\">Storage</b><br>\r\n</p>\r\n<div class=\"row row-no-gutters\">\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Storage Duration (Estimated):</div>\r\n    <div class=\"col-xs-3\">{{ doc.storage_duration }}</div>\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Last Day to Clear (Estimated):</div>\r\n    <div class=\"col-xs-3\">{{ doc.last_day_to_clear }}</div>\r\n</div>\r\n<div class=\"row row-no-gutters\">\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Usage Date (Estimated):</div>\r\n    <div class=\"col-xs-3\">{{ doc.usage_date }}</div>\r\n</div>\r\n<div class=\"row row-no-gutters\">\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Handling Method:</div>\r\n    <div class=\"col-xs-9\">{{ doc.handling_method }}</div>\r\n</div>\r\n<div class=\"row row-no-gutters\">\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Storage Criteria and Location:</div>\r\n    <div class=\"col-xs-9\">{{ doc.storage_criteria_and_location }}</div>\r\n</div>\r\n<div class=\"row row-no-gutters\">\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Estimated Delivery Time:</div>\r\n    <div class=\"col-xs-3\">{{ doc.storage_criteria_and_location }}</div>\r\n</div>\r\n<div class=\"row row-no-gutters\">\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Forwarder Contact Name:</div>\r\n    <div class=\"col-xs-3\">{{ doc.forwarder_contact_name }}</div>\r\n    <div class=\"col-xs-3\" style=\"color: gray;\">Forwarder Contact No.:</div>\r\n    <div class=\"col-xs-3\">{{ doc.forwarder_contact_phone }}</div>\r\n</div>\r\n<p style=\"margin-top: 1rem; margin-bottom: 0.3rem\">\r\n\tApproved By {{get_approver_name(doc, \"Approved\", \"full_name\") }}\r\n</p>\r\n<div style=\"margin-top: 1rem\"><b style=\"margin-top: 1rem\">List of Attachments:</b><br></div>\r\n{% set files = frappe.get_all('File', filters={'attached_to_doctype': 'Permit to Work', 'attached_to_name': doc.name}, fields=['file_name', 'file_url']) %}\r\n<ul>\r\n{%- for file in files -%}\r\n    <li>{{ file.file_name }}</li>\r\n{%- endfor -%} \r\n</ul>",
