                            "qtyrm": 180.0
                          },
                          {
                            "rawMaterialId": 40,
                            "rawMaterialRefNo": "RM-NS-NSA",
                            "rawMaterialName": "Nutrient A",
                            "uomrm": "ml",
                            "uomProduct": "Per Tray",
                            "qtyrm": 180.0
                          },
                          {
                            "rawMaterialId": 41,
                            "rawMaterialRefNo": "RM-NS-NSB",
                            "rawMaterialName": "Nutrient B",
                            "uomrm": "ml",
                            "uomProduct": "Per Tray",
                            "qtyrm": 180.0
                          },
                          {
                            "rawMaterialId": 57,
                            "rawMaterialRefNo": "RM-SD-SPC",
